{
  "name": "@verana-labs/ts-client-test",
  "version": "0.1.0",
  "description": "TypeScript client test harness for Verana blockchain",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test:create-tr": "npx tsx src/journeys/createTrustRegistry.ts",
    "test:update-tr": "npx tsx src/journeys/updateTrustRegistry.ts",
    "test:archive-tr": "npx tsx src/journeys/archiveTrustRegistry.ts",
    "test:add-gf-doc": "npx tsx src/journeys/addGovernanceFrameworkDocument.ts",
    "test:increase-gf-version": "npx tsx src/journeys/increaseActiveGovernanceFrameworkVersion.ts",
    "test:add-did": "npx tsx src/journeys/addDID.ts",
    "test:renew-did": "npx tsx src/journeys/renewDID.ts",
    "test:remove-did": "npx tsx src/journeys/removeDID.ts",
    "test:touch-did": "npx tsx src/journeys/touchDID.ts",
    "test:create-cs": "npx tsx src/journeys/createCredentialSchema.ts",
    "test:update-cs": "npx tsx src/journeys/updateCredentialSchema.ts",
    "test:archive-cs": "npx tsx src/journeys/archiveCredentialSchema.ts",
    "test:all": "npx tsx src/journeys/runAll.ts"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.32.4",
    "@cosmjs/crypto": "^0.32.4",
    "@cosmjs/encoding": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/tendermint-rpc": "^0.32.4",
    "long": "5.3.2",
    "protobufjs": "7.2.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.3"
  }
}

