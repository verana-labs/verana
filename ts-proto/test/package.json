{
  "name": "@verana-labs/ts-client-test",
  "version": "0.1.0",
  "description": "TypeScript client test harness for Verana blockchain",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test:de-grant-auth": "npx tsx src/journeys/deGrantOperatorAuthorization.ts",
    "test:tr-create": "npx tsx src/journeys/trCreateTrustRegistry.ts",
    "test:tr-add-gfd": "npx tsx src/journeys/trAddGovernanceFrameworkDocument.ts",
    "test:tr-increase-gf-version": "npx tsx src/journeys/trIncreaseActiveGovernanceFrameworkVersion.ts",
    "test:tr-update": "npx tsx src/journeys/trUpdateTrustRegistry.ts",
    "test:tr-archive": "npx tsx src/journeys/trArchiveTrustRegistry.ts",
    "test:all": "npx tsx src/journeys/runAll.ts"
