    "test:setup-perm-session-perms": "npx tsx src/journeys/setupPermissionSessionPermissions.ts",
    "test:create-perm-session": "npx tsx src/journeys/createOrUpdatePermissionSession.ts",
    "test:reclaim-td-yield": "npx tsx src/journeys/reclaimTrustDepositYield.ts",
    "test:reclaim-td": "npx tsx src/journeys/reclaimTrustDeposit.ts",
    "test:all": "npx tsx src/journeys/runAll.ts"
